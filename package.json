{
    "name": "mieciekbot-discord-bot",
    "version": "5.0.0",
    "main": "src/index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "build:watch": "tsc -w",
        "start": "node dist/src/index.js",
        "start:dev:js": "node --require dotenv/config --inspect=9229 dist/src/index.js",
        "start:dev:ts": "node --require ts-node/register --require dotenv/config --inspect=9229 src/index.ts",
        "dev:js": "nodemon -e js --exec npm run start:dev:js",
        "dev:ts": "nodemon -e ts --exec npm run start:dev:ts",
        "clean": "rm -rf dist/"
    },
    "dependencies": {
        "@discordjs/builders": "^0.6.0",
        "@discordjs/opus": "^0.6.0",
        "@discordjs/rest": "^0.1.0-canary.0",
        "@discordjs/voice": "^0.6.0",
        "axios": "^0.21.4",
        "bufferutil": "^4.0.3",
        "canvas": "^2.8.0",
        "discord-api-types": "^0.23.1",
        "discord.js": "^13.1.0",
        "emoji-regex": "^9.2.2",
        "mongoose": "^6.0.5",
        "octonode": "^0.10.2",
        "random-number-csprng": "^1.0.2",
        "random-puppy": "^1.1.0",
        "sodium": "^3.0.2",
        "utf-8-validate": "^5.0.5",
        "youtube-search": "^1.1.6",
        "ytdl-core": "^4.9.1"
    },
    "devDependencies": {
        "@types/node": "^16.9.0",
        "dotenv": "^10.0.0",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.2"
    },
    "author": "Maciej Opali≈Ñski <poprostumieciek@gmail.com>",
    "repository": {
        "url": "https://github.com/PoProstuMieciek/MieciekBot.git"
    },
    "license": "GPL-3.0"
}
